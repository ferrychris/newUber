  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "verify": "run-p verify:*",
    "verify:eslint": "eslint --max-warnings 0 --ext .tsx,.ts,.js,.jsx ./src",
    "verify:prettier": "prettier -c --ignore-path .gitignore .",
    "verify:tsc": "tsc --noEmit",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml}\"",
    "preview": "vite preview",
